{
  "name": "@agent-base/api-client",
  "version": "0.18.0",
  "type": "module",
  "description": "Typed API clients for inter-service communication",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./database-clients": {
      "import": "./dist/database-clients/index.js",
      "require": "./dist/database-clients/index.js",
      "types": "./dist/database-clients/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx rimraf dist && tsc",
    "dev": "tsc --watch",
    "clean": "npx rimraf dist .turbo node_modules",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-base/types": "workspace:*",
    "ai": "^4.3.12",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@kit/eslint-config": "workspace:*",
    "@kit/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@agent-base/types": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}