{
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "installCommand": "npm i -g pnpm@8.15.8 && pnpm install",
  "outputDirectory": ".next",
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 