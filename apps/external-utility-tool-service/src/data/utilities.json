[
  {
    "id": "stripe_list_refunds",
    "utilityProvider": "stripe",
    "description": "Lists refunds from a Stripe account, with optional filtering parameters.",
    "schema": {
      "properties": {
        "limit": {
          "type": "number",
          "description": "A limit on the number of objects to be returned. Limit can range between 1 and 100, and the default is 10.",
          "examples": [
            10,
            25
          ]
        },
        "starting_after": {
          "type": "string",
          "description": "A cursor for use in pagination. starting_after is an object ID that defines your place in the list.",
          "examples": [
            "re_1234567890"
          ]
        },
        "ending_before": {
          "type": "string",
          "description": "A cursor for use in pagination. ending_before is an object ID that defines your place in the list.",
          "examples": [
            "re_1234567890"
          ]
        },
        "charge": {
          "type": "string",
          "description": "Only return refunds for the charge specified by this charge ID.",
          "examples": [
            "ch_1234567890"
          ]
        },
        "payment_intent": {
          "type": "string",
          "description": "Only return refunds for the payment intent specified by this payment intent ID.",
          "examples": [
            "pi_1234567890"
          ]
        }
      },
      "type": "object"
    },
    "authMethod": "API_KEY",
    "requiredSecrets": [
      "api_secret_key"
    ],
    "apiKeyDetails": {
      "secretName": "api_secret_key",
      "scheme": "Bearer"
    },
    "apiDetails": {
      "baseUrl": "https://api.stripe.com/v1",
      "method": "GET",
      "pathTemplate": "/refunds",
      "paramMappings": {
        "query": {
          "limit": "limit",
          "starting_after": "starting_after",
          "ending_before": "ending_before",
          "charge": "charge",
          "payment_intent": "payment_intent"
        }
      },
      "staticHeaders": {
        "Content-Type": "application/x-www-form-urlencoded"
      }
    }
  },
