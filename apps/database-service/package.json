{
  "name": "@helloworld/database-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts --env-file .env",
    "dev:no-watch": "tsx src/index.ts --env-file .env",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "migrate:create": "pnpm exec dotenv -e .env -- node-pg-migrate create",
    "migrate:up": "pnpm exec dotenv -e .env -- node-pg-migrate up",
    "migrate:down": "pnpm exec dotenv -e .env -- node-pg-migrate down",
    "db:seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "@agent-base/types": "workspace:*",
    "@types/uuid": "^10.0.0",
    "ai": "4.3.15",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "express": "^5.1.0",
    "lucide-react": "^0.510.0",
    "node-pg-migrate": "^8.0.1",
    "pg": "^8.16.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/pg": "^8.15.1",
    "@types/supertest": "^6.0.3",
    "axios": "^1.9.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}