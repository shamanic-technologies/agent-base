SESSION_SECRET=REPLACE_WITH_A_STRONG_RANDOM_STRING_FOR_SESSIONS

# Redirection & Cookies
ALLOWED_REDIRECT_DOMAINS=http://localhost:5173,https://your-client-app.com
COOKIE_DOMAIN=localhost

