  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
