{
  "name": "@helloworld/utility-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "node ./tests/run-all.js",
    "test:integration": "tsx tests/index.ts",
    "test:simple": "node tests/integration/utility-service.test.js",
    "test:xata": "node tests/integration/xata-helloworld.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "railway:build": "curl -fsSL https://get.pnpm.io/install.sh | sh - && pnpm install --no-frozen-lockfile && pnpm build"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.15",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.55",
    "@octokit/plugin-retry": "^7.1.4",
    "@octokit/plugin-throttling": "^9.4.0",
    "@octokit/rest": "^21.1.1",
    "@xata.io/client": "^0.30.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "simple-git": "^3.27.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}